event onthemechanged;call super::onthemechanged;choose case eventFlag
	case theme.EVT_THEME,theme.EVT_BKGNDCOLORSTYLE,theme.EVT_ITEMBKGNDSTYLE
		uo_1.theme.of_SetBkgndStyle(theme.#ItemBkgndStyle)
		uo_2.theme.of_SetBkgndStyle(theme.#ItemBkgndStyle)
		uo_1.theme.of_SetItemBkgndStyle(theme.#ItemBkgndStyle)
		uo_2.theme.of_SetItemBkgndStyle(theme.#ItemBkgndStyle)
		if HiByte(HiWord(theme.#BkgndColorStyle)) > 0 then
			uo_1.theme.of_SetBkgndColorStyle(theme.#BkgndColorStyle)
			uo_2.theme.of_SetBkgndColorStyle(theme.#BkgndColorStyle)
		else
			if theme.#BKgndColorStyle < theme.CUSTOM then
				uo_1.theme.of_SetBkgndColorStyle(theme.#BkgndColorStyle)
				uo_2.theme.of_SetBkgndColorStyle(theme.#BkgndColorStyle)
			end if
		end if
end choose
end event

event other;call super::other;Int zDelta

choose case message.number
	case WinMSG.WM_MOUSEWHEEL
		zDelta = HiWord(wparam)
		if zDelta > 0 then
			if zDelta / WinMsg.WHEEL_DELTA > 1 then
				Send(Handle(this),WinMSG.WM_VSCROLL,WinMsg.SB_PAGEUP,0)
			else
				Send(Handle(this),WinMSG.WM_VSCROLL,WinMsg.SB_LINEUP,0)
			end if
		else
			if Abs(zDelta) / WinMsg.WHEEL_DELTA > 1 then
				Send(Handle(this),WinMSG.WM_VSCROLL,WinMsg.SB_PAGEDOWN,0)
			else
				Send(Handle(this),WinMSG.WM_VSCROLL,WinMsg.SB_LINEDOWN,0)
			end if
		end if
end choose
end event

event resize;call super::resize;uo_1.width = newwidth - uo_1.x  - 10
uo_2.height = height - uo_2.y - 10
end event

