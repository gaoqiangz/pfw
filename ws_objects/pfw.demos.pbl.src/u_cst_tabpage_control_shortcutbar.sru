event onthemechanged;call super::onthemechanged;int index,count
DragObject obj
u_cst_buttonlistbar u_Buttonlist

count = of_GetCount()
choose case eventflag
	case theme.EVT_BKGNDCOLORSTYLE
		for index = 1 to count
			if IsSucceeded(of_GetObject(index,ref obj)) then
				if ClassName(obj) = "u_cst_tabpage_control_shortcutbar_blb"  then
					u_Buttonlist = obj
					u_Buttonlist.theme.of_SetBkgndColorStyle(theme.#BkgndColorStyle)
				end if
			end if
		next
	case theme.EVT_BKGNDSTYLE
		for index = 1 to count
			if IsSucceeded(of_GetObject(index,ref obj)) then
				if ClassName(obj) = "u_cst_tabpage_control_shortcutbar_blb" then
					u_Buttonlist = obj
					u_Buttonlist.theme.of_SetBkgndStyle(theme.#BkgndStyle)
				end if
			end if
		next
end choose
end event

